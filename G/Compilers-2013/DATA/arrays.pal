program Arrays;
   
procedure main()
var i : int;
    j : int;
    k : int;
    a : array of array of int;
begin
    // check that input is valid
    n := 4;
    if n < 1 then return;

    // fill array
    a := new(2,3);

    i := 0;
    j := 0;

    while i < 2 do
    begin
      while j < 3 do
      begin
        a[i,j] := i * j;
        write( a[i,j] );
        j := j + 1;
        if j < 3 then write(',');
      end;
      i := i + 1;
      if i < 2 then write(',');
    end;

end; // main
