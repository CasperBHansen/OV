
program CallByValueResult;

procedure f(a : int; b : int)
begin
	a := a + 1;
	b := b + 1;
end;

procedure main()
var
	x : int;
	y : int;
begin
	x := 2;
	y := 3;
	
	write("Input:\n");
	write("x: "); write(x);
	write('\n');
	write("y: "); write(y);
	write('\n'); write('\n');

	f(x, y);

	write("Output:\n");
	write("x: "); write(x);
	write('\n');
	write("y: "); write(y);
	write('\n');
end;